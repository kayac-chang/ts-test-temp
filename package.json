{
  "name": "ts-test-temp",
  "version": "0.0.1",
  "description": "A test project for vitest + typescript + eslint + prettier",
  "scripts": {
    "test": "vitest run",
    "format": "prettier --write src",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "prettier": "^2.8.8",
    "typescript": "5.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "prettier": {
    "semi": true,
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "always",
    "trailingComma": "es5"
  }
}
